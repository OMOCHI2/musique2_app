<% provide(:title, @title) %>
<div class="row">
  <%= render "user_info_side" %>

  <div class="col-md-8">
    <h3><%= @title %></h3>
    <% if @users.any? %>
      <ul class="users follow list-unstyled">
        <%= render @users %>
      </ul>
      <%= will_paginate %>
    <% end %>
  </div>
</div>
